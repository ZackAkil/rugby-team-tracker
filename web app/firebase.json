{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/match/create",
        "destination": "/create.html"
      },
      {
        "source": "/match/list",
        "destination": "/match_directory.html"
      },
      {
        "source": "/match/*",
        "destination": "/match.html"
      },
      {
        "source": "/match/*/edit",
        "destination": "/edit.html"
      }
    ]
  }
}
